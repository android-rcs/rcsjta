<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    
	<TableLayout android:layout_width="fill_parent"
	  android:layout_height="wrap_content"
	  android:layout_margin="5dip"
	  android:stretchColumns="0,1">
	    <TableRow android:layout_width="fill_parent"
	      android:layout_height="wrap_content">
		<Button android:text="@string/label_save"
		 android:layout_width="fill_parent"
		 android:layout_height="wrap_content"
		 android:id="@+id/save_btn"/>    
		<Button android:text="@string/label_generate_profile"
		 android:layout_width="fill_parent"
		 android:layout_height="wrap_content"
		 android:id="@+id/gen_btn"/>    
	    </TableRow>
	</TableLayout> 	    
	
	<ScrollView android:layout_width="fill_parent" android:layout_height="fill_parent">
	
	<LinearLayout
	    android:orientation="vertical"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent">

		<TextView
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
		android:text="@string/GSMA_release" />
		<EditText android:id="@+id/release"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:enabled="false"/>
        
       	<TextView android:text="@string/IMS_authentication_procedure_for_Mobile_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/ImsAuthenticationProcedureForMobile"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

       	<TextView android:text="@string/IMS_authentication_procedure_for_Wi_Fi_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<Spinner android:id="@+id/ImsAuthenticationProcedureForWifi"
		android:layout_height="wrap_content"
		android:layout_width="fill_parent"
		android:layout_marginLeft="5dip"
		android:layout_marginRight="5dip"
		android:drawSelectorOnTop="true"/>

		<TextView android:text="@string/IMS_username"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsUsername"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/IMS_display_name"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsDisplayName"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/IMS_home_domain"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsHomeDomain"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/IMS_private_URI_for_HTTP_Digest"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsPrivateId"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/IMS_password_for_HTTP_Digest"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsPassword"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/IMS_realm_for_HTTP_Digest"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsRealm"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/P_CSCF_address_for_Mobile_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyAddrForMobile"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"/>

       	<TextView android:text="@string/P_CSCF_port_for_Mobile_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyPortForMobile"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5"
	 	android:inputType="number"/>

	 	<TextView android:text="@string/P_CSCF_address_for_Wi_Fi_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyAddrForWifi"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"/>

	 	<TextView android:text="@string/P_CSCF_port_for_Wi_Fi_access"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImsOutboundProxyPortForWifi"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="5"
	 	android:inputType="number"/>

	 	<TextView android:text="@string/XDM_server_address"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/XdmServerAddr"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"/>

       	<TextView android:text="@string/XDM_server_login"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/XdmServerLogin"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/XDM_server_password"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/XdmServerPassword"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

		<TextView android:text="@string/FT_HTTP_server_address"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/FtHttpServerAddr"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"/>

       	<TextView android:text="@string/FT_HTTP_server_login"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/FtHttpServerLogin"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/FT_HTTP_server_password"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/FtHttpServerPassword"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>
		
       	<TextView android:text="@string/IM_conference_URI"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/ImConferenceUri"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/End_user_confirmation_URI"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/EndUserConfReqUri"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"
	 	android:maxLength="100"/>

       	<TextView android:text="@string/APN"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>
		<EditText android:id="@+id/RcsApn"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	 	android:layout_marginLeft="5dip"
	 	android:layout_marginRight="5dip"/>

       

	 	<TextView android:text="@string/Capabilities"
	    android:paddingTop="5dip"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"/>

		<CheckBox android:id="@+id/image_sharing"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
		android:text="@string/Image_sharing" />

        <CheckBox android:id="@+id/video_sharing"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/Video_sharing" />

        <CheckBox android:id="@+id/file_transfer"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/File_transfer" />

        <CheckBox android:id="@+id/file_transfer_http"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/File_transfer_HTTP" />
        
        <CheckBox android:id="@+id/im"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="@string/IM_session" />
        
        <CheckBox android:id="@+id/im_group"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="@string/IM_Group_session" />
        
        <CheckBox android:id="@+id/ipvoicecall"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:enabled="true"
        android:text="@string/IP_Voice_call" />
        
        <CheckBox android:id="@+id/ipvideocall"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
        android:enabled="true"
        android:text="@string/IP_Video_call" />

        <CheckBox android:id="@+id/cs_video"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/CS_video" />

        <CheckBox android:id="@+id/presence_discovery"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/Presence_discovery" />

		<CheckBox android:id="@+id/social_presence"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:textStyle="bold"
		android:enabled="true"
        android:text="@string/Social_presence" />
		
		<CheckBox android:id="@+id/geolocation_push"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:textStyle="bold"
		android:enabled="true"
        android:text="@string/Geolocation_push" />
		
		<CheckBox android:id="@+id/file_transfer_thumbnail"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/File_transfer_thumbnail" />

		<CheckBox android:id="@+id/file_transfer_sf"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/File_transfer_Store_Forward" />

		<CheckBox android:id="@+id/group_chat_sf"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/Group_chat_Store_Forward" />
		
		<CheckBox android:id="@+id/sip_automata"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
        android:textStyle="bold"
		android:enabled="true"
        android:text="@string/SIP_automata" />

	</LinearLayout>
	</ScrollView>
	
</LinearLayout>
    